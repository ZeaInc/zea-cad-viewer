{
  "name": "zea-cad-viewer",
  "version": "0.0.1",
  "license": "MIT",
  "dependencies": {
    "@zeainc/gltf-loader": "^4.0.5",
    "@zeainc/zea-engine": "4.9.0-b37f79b",
    "@zeainc/zea-ux": "4.4.1"
  },
  "devDependencies": {
    "@auth0/auth0-spa-js": "^1.6.5",
    "@babel/core": "^7.13.1",
    "@rollup/plugin-commonjs": "^15.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^10.0.0",
    "@rollup/plugin-typescript": "^6.0.0",
    "@rollup/plugin-terser": "^0.4.3",
    "@roxi/routify": "^2.15.1",
    "@tsconfig/svelte": "^1.0.0",
    "babel-loader": "^8.2.2",
    "cross-env": "^7.0.2",
    "cssnano": "^4.1.10",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.2.8",
    "postcss-import": "^14.0.0",
    "prettier": "^2.2.1",
    "prettier-plugin-svelte": "^2.2.0",
    "rollup": "^2.79.2",
    "rollup-plugin-hot": "^0.1.1",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-svelte": "^6.1.0",
    "sha256": "^0.2.0",
    "spank": "^1.4.0",
    "spassr": "^2.4.0",
    "svelte": "^3.35.0",
    "svelte-accessible-dialog": "^2.1.2",
    "svelte-check": "^1.0.0",
    "svelte-loader": "^3.0.0",
    "svelte-preprocess": "^4.6.9",
    "tailwindcss": "^2.0.4",
    "tslib": "^2.0.0",
    "typescript": "^3.9.3"
  },
  "browser": {
    "crypto": false
  },
  "scripts": {
    "build": "run-s build:*",
    "build:app": "routify -b && rollup -c",
    "build:static": "spank",
    "dev": "run-p routify rollup",
    "rollup": "rollup -cw",
    "routify": "routify",
    "serve": "spassr --ssr",
    "to-cleanup": "rm -Rf .routify/ dist/ node_modules/",
    "validate": "svelte-check"
  },
  "packageManager": "yarn@1.22.19",
  "routify": {
    "extensions": "svelte,html,svx,md"
  },
  "spassr": {
    "assetsDir": [
      "dist",
      "assets"
    ],
    "script": "dist/build/main.js"
  },
  "spank": {
    "blacklist": [
      "/example/modal/basic/4"
    ]
  }
}
